function my_DCT_compressor(X)
    N_list = [4,8,16,32];
    Z_list = [X, X, X, X];
    
    pause(2)
    display(Z_list(1))
    pause(4)
    
    for i = 1:length(N_list)
        N = N_list(i)
        step = 1;
        bit_length = 1000000;
        while bit_length > 50000,
            try
                vlc = jpegenc(X-128,step,N);
                Z = jpegdec(vlc,step,N);
                bit_length = sum(vlc(:,2))
            catch ME
                display("error coding with step size")
            end
        display(bit_length)
        step = step + 1;
        end
    end
    
    draw(beside(Z(1),beside(Z(2),beside(Z(3),Z(4)))))